# Airtable Configuration
# This should be a read-only token scoped to the unified db
AIRTABLE_PAT=your_airtable_personal_access_token

# HCB OAuth Configuration
# This should be a read-only token with api/v4 access
HCB_APP_UID=your_hcb_app_uid
HCB_APP_SECRET=your_hcb_app_secret
HCB_REDIRECT_URI=http://localhost:3000

# Application Configuration
PORT=3000
